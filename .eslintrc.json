{
	"extends": "airbnb",
	"rules": {
		"semi": ["error", "never"],
		"quotes": [2, "single"],
		"import/first": ["error", "ignore"],
		"no-tabs": 0,
		"import/no-unresolved": 0,
		"import/extensions": 0,
		"react/prefer-stateless-function": 0,
		"react/jsx-filename-extension": 0,
		"react/forbid-prop-types": 0,
		"arrow-parens": ["error", "as-needed"],
		"jsx-a11y/img-redundant-alt": 0
	}
}
